{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "jsx": "react-jsx",
        "jsxImportSource": "preact",
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "src/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@data/*": [
                "src/data/*"
            ],
            "@styles/*": [
                "src/styles/*"
            ],
            "@types/*": [
                "src/types/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ]
        }
    }
}